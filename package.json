{
  "name": "@charliewilco/immutable-array-helpers",
  "version": "0.1.0",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest --runInBand --verbose",
    "test:ci": "yarn test --ci --no-cache",
    "build": "rollup -c",
    "prepare": "yarn build",
    "dev": "yarn build --watch"
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "tslint": "^5.19.0",
    "esbuild": "^0.13.13",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-esbuild": "^4.6.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "prettier": {
    "printWidth": 85,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
